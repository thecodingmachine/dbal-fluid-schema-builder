{
    "name": "thecodingmachine/dbal-fluid-schema-builder",
    "description": "Build and modify your database schema using Doctrine DBAL and a fluid syntax.",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.3",
        "doctrine/inflector": "^1.4.3 || ^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\FluidSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\FluidSchema\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
